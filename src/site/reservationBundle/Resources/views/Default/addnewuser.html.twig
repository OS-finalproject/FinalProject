{# src/Acme/TaskBundle/Resources/views/Default/new.html.twig #}
{% extends '::base.html.twig' %}
{% block title %}Index{% endblock %}
{% block body %}
	<div class="row">
						<div class="12u">
							<!-- Banner -->
								<section class="box">
<div>
<form action="{{ path('sitereservation_addnewuserform') }}" method="post" {{ form_enctype(form) }}>
<table>

    <tr>
    <td>{{ form_label(form.username) }}</td>
    <td> {{ form_errors(form.username) }}
    {{ form_widget(form.username) }}</td>
    </tr>
    <tr>
    <td>{{ form_label(form.email) }}</td>
    <td> {{ form_errors(form.email) }}
    {{ form_widget(form.email) }}</td>
    </tr>
    <tr>
    <td>{{ form_label(form.password) }}</td>
    <td> {{ form_errors(form.password) }}
    {{ form_widget(form.password) }}</td>
    </tr>
    <tr>
    <td>{{ form_label(form.file) }}</td>
    <td> {{ form_errors(form.file) }}
    {{ form_widget(form.file) }}</td>
    </tr>   
    <tr><td colspan='2'>{{ form_rest(form) }}</td></tr>
    <tr><td colspan='2'><center><br><br><input type="submit" value="Save"/></center></td></tr>
    </table>
</form>
</div>
								</section>
						</div>
		
{% endblock %}