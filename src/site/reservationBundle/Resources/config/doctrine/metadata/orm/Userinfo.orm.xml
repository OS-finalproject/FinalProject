<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Userinfo" table="userinfo">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="255" nullable="false"/>
    <field name="email" type="string" column="email" length="255" nullable="false"/>
    <field name="password" type="string" column="password" length="255" nullable="false"/>
    <field name="address" type="text" column="address" nullable="false"/>
    <field name="telephone" type="text" column="telephone" nullable="false"/>
    <field name="mobile" type="text" column="mobile" nullable="false"/>
    <field name="numChildren" type="integer" column="num_children" nullable="false"/>
    <field name="gendar" type="string" column="gendar" nullable="false"/>
    <field name="image" type="text" column="image" nullable="false"/>
  </entity>
</doctrine-mapping>
